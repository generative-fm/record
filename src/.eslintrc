{
  "extends": "@alexbainter/eslint-config/react",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": "2020"
  },
  "globals": {
    "process": "readonly"
  },
  "overrides": [
    {
      "files": ["**/*.spec.js"],
      "env": {
        "mocha": true
      },
      "globals": {
        "expect": "readonly"
      }
    }
  ]
}
